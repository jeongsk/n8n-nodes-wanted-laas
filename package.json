{
	"name": "n8n-nodes-wanted-laas",
	"version": "0.1.13",
	"description": "Wanted LaaS nodes for n8n",
	"keywords": [
		"n8n-community-node-package",
		"n8n-nodes-wanted-laas",
		"wanted",
		"laas"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "jeongsk",
		"email": "jeongseok@wantedlab.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jeongsk/n8n-nodes-wanted-laas.git"
	},
	"engines": {
		"node": ">=20.15",
		"pnpm": ">=9.1"
	},
	"packageManager": "pnpm@9.1.4",
	"main": "index.js",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"build": "tsc && gulp build:icons",
		"build:dev": "pnpm build && docker-compose up --build --force-recreate",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/WantedLaaSApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/WantedLaaS/WantedLaaS.node.js",
			"dist/nodes/WantedLaaS/LmChatWantedLaaS.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "~8.32.0",
		"eslint": "^8.57.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*",
		"@langchain/openai": "*"
	}
}
